{
  "server": {
    "name": "{{server_name}}",
    "version": "{{version}}",
    "description": "{{description}}"
  },
  "mental_models": {
    "enabled": true,
    "default_models": ["{{default_model_1}}", "{{default_model_2}}", "{{default_model_3}}"],
    "custom_models_path": "./mental_models"
  },
  "sequential_thinking": {
    "max_thoughts": 10,
    "allow_revisions": true,
    "allow_branching": true,
    "track_uncertainty": true,
    "verification_required": true
  },
  "integration": {
    "knowledge_graph": {
      "enabled": true,
      "sync_interval": 300
    },
    "token_manager": {
      "enabled": true,
      "optimize_prompts": true
    },
    "agent_forge": {
      "enabled": true,
      "workflow_integration": true
    }
  },
  "templates": {
    "path": "./templates",
    "auto_select": true,
    "default_template": "{{default_template}}"
  },
  "logging": {
    "level": "info",
    "file": "{{server_name}}.log",
    "include_timestamps": true
  },
  "debug": {
    "enabled": false,
    "verbose": false,
    "save_thought_process": true
  }
}
